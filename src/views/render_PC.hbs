<body>
	<div class="title">
		<h1>View Your Character</h1>
</div>
<div class="charTable">
{{#each PCs }}
    <h1>Name: {{PC_name}}</h1>
    <h1>Level: {{PC_level}} <input type="text" name="level"><button type="button" class="1">Update</button></h1>
    <h1>Race: {{Race_name}}</h1>
    <h1>Class: {{Class_name}}</h1>
	<h1>Armor Class: {{Armor_Class}} <input type="text" name="armor"><button type="button" class="1">Update</button></h1>
	<h1>Strength: {{Strength}} <input type="text" name="strength"><button type="button" class="2">Update</button></h1>
	<h1>Dexterity: {{Dexterity}} <input type="text" name="dex"><button type="button" class="3">Update</button></h1>
	<h1>Wisdom: {{Wisdom}} <input type="text" name="wisdom"><button type="button" class="4">Update</button></h1>
	<h1>Intelligence: {{Intelligence}} <input type="text" name="intell"><button type="button" class="5">Update</button></h1>
	<h1>Constitution: {{Constitution}} <input type="text" name="const"><button type="button" class="6">Update</button></h1>
	<h1>Charisma: {{Charisma}} <input type="text" name="charisma"><button type="button" class="7">Update</button></h1>
	<h1>Max HP: {{Max_HP}} <input type="text" name="maxhp"><button type="button" class="8">Update</button></h1>
	<h1>Current HP: {{Current_HP}} <input type="text" name="currhp"><button type="button" class="9">Update</button></h1>
    <h1>Description: {{Notes}}</h1>

{{/each}}

</div>
	<div class="button">
		<button type="button"><a href="/">Home</a></button>
	</div>
</body>
